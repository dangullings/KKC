<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="457.0" prefWidth="711.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.NewTestController">
   <children>
      <BorderPane prefHeight="457.0" prefWidth="711.0">
         <center>
            <TableView fx:id="testStudentsTable" prefHeight="349.0" prefWidth="360.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="10.0" top="10.0" />
               </BorderPane.margin></TableView>
         </center>
         <bottom>
            <HBox prefHeight="40.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#pressCancel" prefHeight="60.0" prefWidth="288.0" text="Cancel" />
                  <Button fx:id="btnSave" alignment="CENTER" mnemonicParsing="false" onAction="#saveTest" prefHeight="60.0" prefWidth="312.0" text="Save" />
               </children>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
            </HBox>
         </bottom>
         <top>
            <HBox prefHeight="48.0" prefWidth="600.0" spacing="50.0">
               <children>
                  <DatePicker fx:id="datePicker" onAction="#setTestDate" promptText="test date" />
                  <CheckBox fx:id="checkIsBlackbelt" mnemonicParsing="false" onAction="#typeChanged" prefHeight="49.0" prefWidth="148.0" text="Blackbelt" />
                  <ChoiceBox fx:id="choiceLocation" prefWidth="150.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </top>
         <left>
            <VBox prefHeight="349.0" prefWidth="176.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="studentsTable" prefHeight="299.0" prefWidth="194.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></TableView>
                  <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#pressAdd" prefHeight="25.0" prefWidth="176.0" text="Add Student(s)">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Button>
                  <Button fx:id="btnRemove" mnemonicParsing="false" prefHeight="26.0" prefWidth="175.0" text="Remove Student">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Button>
               </children>
               <BorderPane.margin>
                  <Insets left="5.0" right="5.0" top="10.0" />
               </BorderPane.margin></VBox>
         </left>
         <right>

            
         </right>
         <right>
            <HBox prefHeight="359.0" prefWidth="137.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label prefHeight="26.0" prefWidth="100.0" text="Form" />
                        <Label layoutX="10.0" layoutY="10.0" prefHeight="27.0" prefWidth="104.0" text="Steps" />
                        <Label layoutX="10.0" layoutY="37.0" prefHeight="27.0" prefWidth="104.0" text="Power" />
                        <Label layoutX="10.0" layoutY="64.0" prefHeight="27.0" prefWidth="104.0" text="Kiap" />
                        <Label layoutX="10.0" layoutY="91.0" prefHeight="27.0" prefWidth="104.0" text="Questions" />
                        <Label layoutX="10.0" layoutY="118.0" prefHeight="27.0" prefWidth="104.0" text="Attitude" />
                        <Label layoutX="10.0" layoutY="145.0" prefHeight="27.0" prefWidth="104.0" text="Sparring" />
                        <Label layoutX="10.0" layoutY="172.0" prefHeight="27.0" prefWidth="104.0" text="Breaking" />
                     </children></VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                        <TextField fx:id="txtForm" promptText="Form" />
                        <TextField fx:id="txtSteps" promptText="Steps" />
                        <TextField fx:id="txtPower" promptText="Power" />
                        <TextField fx:id="txtKiap" promptText="Kiap" />
                        <TextField fx:id="txtQuestions" promptText="Questions" />
                        <TextField fx:id="txtAttitude" promptText="Attitude" />
                        <TextField fx:id="txtSparring" promptText="Sparring" />
                        <TextField fx:id="txtBreaking" promptText="Breaking" />
                     </children>
                  </VBox>
               </children>
               <BorderPane.margin>
                  <Insets right="5.0" />
               </BorderPane.margin>
            </HBox>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
